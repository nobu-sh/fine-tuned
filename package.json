{
  "name": "fine-tuned",
  "version": "0.0.1",
  "description": "A package for playing music with Discord.js",
  "license": "MIT",
  "author": "nobu-sh",
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "commit": "commit",
    "format": "prettier --write **/*.{ts,js,json,yml,yaml}",
    "lint": "eslint packages --ext mjs,js,ts",
    "lint:fix": "eslint packages --ext mjs,js,ts --fix",
    "prepack": "npm run build",
    "prepare": "husky install",
    "test": "jest --pass-with-no-tests",
    "test:ci": "jest --no-stack-trace --verbose --pass-with-no-tests",
    "watch": "nodemon --watch src --exec \"npm run build\""
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "typings": "./lib/index.d.ts",
  "exports": {
    "import": "./lib/index.mjs",
    "require": "./lib/index.js"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "homepage": "https://github.com/nobu-sh/fine-tuned#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nobu-sh/fine-tuned"
  },
  "bugs": {
    "url": "https://github.com/nobu-sh/fine-tuned/issues"
  },
  "peerDependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.14.0",
    "discord.js": "^14.7.0",
    "ffmpeg-static": "^5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-angular": "^17.4.2",
    "@commitlint/prompt-cli": "^17.4.2",
    "@swc/core": "^1.3.34",
    "@types/colors": "^1.2.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-marine": "^9.4.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "colors": "^1.4.0",
    "libsodium-wrappers": "^0.7.10",
    "node-fetch": "^3.3.0",
    "play-dl": "^1.9.6",
    "prism-media": "^1.3.4",
    "youtube-sr": "^4.3.4"
  }
}
